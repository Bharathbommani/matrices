#include<stdio.h>
int main()
{
int a[3][3];
int b[3][3],c[3][3];
int i,j;
printf("\n Enter elements of 1st Matrix");
 
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
scanf("%d",&a[i][j]);
}
}
printf("\n Enter elements of 2nd Matrix");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
scanf("%d",&b[i][j]);
}
}
printf("\n Result of subtracting two matrices is\n");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
 
c[i][j]=a[i][j]-b[i][j];
}
}
 
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("\t%d",c[i][j]);
}
printf("\n");
}
return 0;
}
